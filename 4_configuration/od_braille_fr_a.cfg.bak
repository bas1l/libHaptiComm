			actuatorArrangement = ["dot4", "dot5"];
			actuatorArrangement2 = ["dot1", "dot2", "dot6"];
		}
		
		uid-symbol {
			id = "≥";
			motion = "silence";
			actuatorArrangement = ["dot4", "dot5"];
			actuatorArrangement2 = ["dot3", "dot4", "dot5"];
		}
		
		uid-symbol {
			id = "[";
			motion = "silence";
			actuatorArrangement = ["dot4", "dot5"];
			actuatorArrangement2 = ["dot2", "dot3", "dot6"];
		}
		
		uid-symbol {
			id = "]";
			motion = "silence";
			actuatorArrangement = ["dot3", "dot5", "dot6"];
			actuatorArrangement2 = ["dot4", "dot5"];
		}
		
		#################################
		# Special Symbols				#
		#################################
		
		uid-symbol {
			id = "©";
			motion = "silence";
			actuatorArrangement = ["dot5"];
			actuatorArrangement2 = ["dot1", "dot4"];
		}
		
		uid-symbol {
			id = "°";
			motion = "silence";
			actuatorArrangement = ["dot5"];
			actuatorArrangement2 = ["dot1", "dot3", "dot5"];
		}
		
		uid-symbol {
			id = "§";
			motion = "silence";
			actuatorArrangement = ["dot5"];
			actuatorArrangement2 = ["dot1", "dot2", "dot3", "dot4"];
		}
		
		uid-symbol {
			id = "®";
			motion = "silence";
			actuatorArrangement = ["dot5"];
			actuatorArrangement2 = ["dot1", "dot2", "dot3", "dot5"];
		}
		
		uid-symbol {
			id = "™";
			motion = "silence";
			actuatorArrangement = ["dot5"];
			actuatorArrangement2 = ["dot2", "dot3", "dot4", "dot5"];
		}
		
		uid-symbol {
			id = "&";
			motion = "silence";
			actuatorArrangement = ["dot5"];
			actuatorArrangement2 = ["dot1", "dot2", "dot3", "dot4", "dot5", "dot6"];
		}
		
		uid-symbol {
			id = "<";
			motion = "silence";
			actuatorArrangement = ["dot5"];
			actuatorArrangement2 = ["dot1", "dot2", "dot6"];
		}
		
		uid-symbol {
			id = ">";
			motion = "silence";
			actuatorArrangement = ["dot5"];
			actuatorArrangement2 = ["dot3", "dot4", "dot5"];
		}
		
		uid-symbol {
			id = "~";
			motion = "silence";
			actuatorArrangement = ["dot5"];
			actuatorArrangement2 = ["dot2", "dot6"];
		}
		
		uid-symbol {
			id = "*";
			motion = "silence";
			actuatorArrangement = ["dot5"];
			actuatorArrangement2 = ["dot3", "dot5"];
		}
		
		uid-symbol {
			id = "\";
			motion = "silence";
			actuatorArrangement = ["dot5"];
			actuatorArrangement2 = ["dot3", "dot4"];
		}
		
		uid-symbol {
			id = "%#";
			motion = "silence";
			actuatorArrangement = ["dot5"];
			actuatorArrangement2 = ["dot3", "dot4", "dot5", "dot6"];
		}
		
		uid-symbol {
			id = "%%";
			motion = "silence";
			actuatorArrangement = ["dot5"];
			actuatorArrangement2 = ["dot3", "dot4", "dot6"];
		}
		
		uid-symbol {
			id = "_";
			motion = "silence";
			actuatorArrangement = ["dot5"];
			actuatorArrangement2 = ["dot3", "dot6"];
		}
		
		
	}#End-Alphabet
    
